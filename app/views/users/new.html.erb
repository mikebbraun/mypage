<h1>Users#New</h1>
<h2>Sign Up</h2>
<%= form_for @user do |f| %>
<p>
  <%= f.label :username %>
  <%= f.text_field :username %>
</p>
<p>
  <%= f.label :password %>
  <%= f.password_field :password %>
</p>
<p>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</p>
<p>
  <%= f.submit %>
</p>
<% end  %>

<%= link_to 'Sign in', new_session_path %>
